{
    "info": {
        "_postman_id": "operadoras-ans-collection",
        "name": "ANS Operadoras API",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Buscar Operadoras por Nome",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://127.0.0.1:8000/operadoras/?nome={{nome}}&limite={{limite}}",
                    "protocol": "http",
                    "host": ["127.0.0.1"],
                    "port": "8000",
                    "path": ["operadoras"],
                    "query": [
                        {
                            "key": "nome",
                            "value": "{{nome}}"
                        },
                        {
                            "key": "limite",
                            "value": "{{limite}}"
                        }
                    ]
                }
            },
            "response": []
        },
        {
            "name": "Buscar Operadora por Registro ANS",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://127.0.0.1:8000/operadora/{{registro_ans}}",
                    "protocol": "http",
                    "host": ["127.0.0.1"],
                    "port": "8000",
                    "path": ["operadora", "{{registro_ans}}"]
                }
            },
            "response": []
        }
    ]
}
